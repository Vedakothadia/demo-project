<div class="container">
    <div class="row">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Orders Id</th>
                    <th scope="col">Address</th>
                    <th scope="col">Time</th>
                </tr>
            </thead>
            <tbody>
                <% if(orders.length){ orders.forEach((order)=>{
                    %>
                    <tr>
                        <th scope="row"><a href="#">
                                <%= order._id%>
                            </a></th>
                        <td>
                            <%= order.address%>
                        </td>
                        <td>
                            <%= moment(order.createdAt).format('hh:mm A')%>
                        </td>
                    </tr>

                    <% }) } else{ %>
                        <tr>
                            <td>No Orders Found!</td>
                        </tr>
                        <% } %>
            </tbody>
        </table>
    </div>
</div>